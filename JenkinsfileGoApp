pipeline {
    agent {
        label 'sandbox'
    }
    stages {
        stage('Build') {
            steps {
                echo "Build Golang Apps"
                sleep(5)
            }
        }
        stage('Test'){
            steps {
                echo "Test Golang Apps"
                sleep(5)
            }
        }
        stage('Deploy') {
            steps {
                echo "Deploy Golang Apps"
            }
        }
    }
}
